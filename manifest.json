{
  "manifest_version": 2,
  "name": "chesscom-reader",
  "version": "0.1",
  "description": "poc",
  "icons": {
    "48": "icons/chess.png"
  },
  "background": {
    "scripts": [
      "src/utils.js",
      "src/background.js"
    ]
  },
  "page_action": {
    "default_icon": "icons/chess.png"
  },
  "permissions": [
    "<all_urls>",
    "activeTab",
    "tabs",
    "*://localhost/*"
  ]
}
